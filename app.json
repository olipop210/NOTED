{
  "expo": {
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Pozwól aplikacji na dostęp do galerii, abyś mógł wybrać zdjęcie ze swoją piękną notatką :)",
          "cameraPermission": "Pozwól aplikacji na dostęp do kamery, abyś mógł sfotografować swoją piękną notatkę :)"
        }
      ], 
      [
        "expo-notifications",
        {
          "icon":"./assets/icon.png"
        }
      ]
    ],
    "name": "Noted",
    "slug": "Noted",
    "version": "Build 021",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/cb2b23af-de49-47c3-a320-77c7dfe03623"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
