      "bundleIdentifier": "com.notedinc.noted",
      "buildNumber": "Build021"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.notedinc.noted",
      "versionCode": 5,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/splash.png"
    },
    "extra": {
      "eas": {
        "projectId": "cb2b23af-de49-47c3-a320-77c7dfe03623"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }, 
    "notification":{
      "color": "#FF0000",
      "icon":"assets/icon.png"
    }
  }
}
